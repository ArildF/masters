		<Configuration
			Name="PDF|Win32"
			OutputDirectory="$(ConfigurationName)"
			IntermediateDirectory="$(ConfigurationName)"
			ConfigurationType="0">
			<Tool
				Name="VCNMakeTool"
				BuildCommandLine="taskkill /im acrord32.exe
make pdf"
				ReBuildCommandLine="taskkill /im acrord32.exe
make clean
make pdf"
				CleanCommandLine="make clean"
				Output="pdf\thesis.pdf"/>
		</Configuration>
		<Configuration
			Name="Chunked HTML|Win32"
			OutputDirectory="$(ConfigurationName)"
			IntermediateDirectory="$(ConfigurationName)"
			ConfigurationType="0">
