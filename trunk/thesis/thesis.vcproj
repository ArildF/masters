	<Configurations>
		<Configuration
			Name="HTML|Win32"
			OutputDirectory="Debug"
			IntermediateDirectory="Debug"
			ConfigurationType="0">
			<Tool
				Name="VCNMakeTool"
				BuildCommandLine="make html"
				ReBuildCommandLine="make clean
make html"
				CleanCommandLine="make clean"
				Output="html\thesis.html"/>
		</Configuration>
		<Configuration
			Name="Release|Win32"
			OutputDirectory="Release"
